{
  "typescript": true,
  "commands": ["./commands", "@adonisjs/core/build/commands", "@adonisjs/lucid/build/commands"],
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Contracts": "contracts",
    "Config": "config"
  },
  "preloads": ["./start/routes", "./start/kernel", "./start/events"],
  "providers": [
    "./providers/AppProvider",
    "@adonisjs/core",
    "@adonisjs/lucid",
    "@adonisjs/redis",
    "@adonisjs/session",
    "@adonisjs/auth",
    "@adonisjs/mail",
    "@adonisjs/view"
  ],
  "metaFiles": [".env", ".adonisrc.json", "resources/views/**/*.edge", "public/**"]
}
